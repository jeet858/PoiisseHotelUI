{
  "name": "react-shopping-cart",
  "version": "0.3.0",
  "private": true,
  "description": "Uvanij",
  "main": "index.js",
  "homepage": ".",
  "proxy": "https://localhost:3000/",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "postbuild": "node iisConfig/copyIISConfig.js && npm run copyCSS",
    "copyCSS": "copyfiles -f ./src/assets/css/%COMPANY%.css ./build/static/css/",
    "env:development": "env-cmd -r %COMPANY%.env-cmdrc -e development",
    "env:qa": "env-cmd -r %COMPANY%.env-cmdrc -e development,qa",
    "env:staging": "env-cmd -r %COMPANY%.env-cmdrc -e development,qa,staging",
    "env:girlco": "env-cmd -r %COMPANY%.env-cmdrc -e development,qa,staging,girlco",
    "env:petindia": "env-cmd -r %COMPANY%.env-cmdrc -e development,qa,staging,girlco,petindia",
    "env:production": "env-cmd -r %COMPANY%.env-cmdrc -e development,qa,staging,production",
    "predeploy": "npm run env:%ENV% npm run build",
    "deploy": "npm run env:%ENV% node deploy.js",
    "serve": "serve -s build",
    "cy": "cypress open",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "**": "*****************DOCS***********************",
    "docs": "jsdoc src -r -d docs",
    "better-docs": "jsdoc -c ./jsdoc.json",
    "***": "**************CODE MODULES******************",
    "frontend": "bash ../bootstrap.sh frontend",
    "backend": "bash ../bootstrap.sh backend",
    "all": "bash ../bootstrap.sh all"
